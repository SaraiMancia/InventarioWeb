<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Index</servlet-name>
        <servlet-class>Controller.Index</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Categorias</servlet-name>
        <servlet-class>Controller.Categorias</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>modificarCategoria</servlet-name>
        <servlet-class>Controller.modificarCategoria</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>categoriasS</servlet-name>
        <servlet-class>Controller.categoriasS</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarCat</servlet-name>
        <servlet-class>Controller.EliminarCat</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>eliminarCategoria</servlet-name>
        <servlet-class>Controller.eliminarCategoria</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductosS</servlet-name>
        <servlet-class>Controller.ProductosS</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>eliminarProducto</servlet-name>
        <servlet-class>Controller.eliminarProducto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>modificarProducto</servlet-name>
        <servlet-class>Controller.modificarProducto</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Index</servlet-name>
        <url-pattern>/Index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Categorias</servlet-name>
        <url-pattern>/Categorias</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>modificarCategoria</servlet-name>
        <url-pattern>/modificarCategoria.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>categoriasS</servlet-name>
        <url-pattern>/categoriasS.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarCat</servlet-name>
        <url-pattern>/EliminarCat.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>eliminarCategoria</servlet-name>
        <url-pattern>/eliminarCategoria.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductosS</servlet-name>
        <url-pattern>/ProductosS.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>eliminarProducto</servlet-name>
        <url-pattern>/eliminarProducto.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>modificarProducto</servlet-name>
        <url-pattern>/modificarProducto.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
